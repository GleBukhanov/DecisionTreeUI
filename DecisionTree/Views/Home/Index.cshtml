@model QuestionModel

<form asp-action="ReturnResult">
    <div>
          <input asp-for="question1" type="checkbox" id="question1" name="question1" checked>
          <label for="question1">Employee answered to the Call?</label>
        </div>
    
    <div>
        <input asp-for="question2" type="checkbox" id="question2" name="question2">
        <label for="question2">Does he have a good CV?</label>
    </div>
    
    <div>
        <input asp-for="question3" type="checkbox" id="question3" name="question3">
        <label for="question3">Is he go to interview?</label>
    </div>
    
    <div>
        <input asp-for="question4" type="checkbox" id="question4" name="question4">
        <label for="question4">Do you have a success interview?</label>
    </div>
    
    <div>
        <input asp-for="question5" type="checkbox" id="question5" name="question5">
        <label for="question5">Does he have required documents?</label>
    </div>
    
    <div>
        <input asp-for="question6" type="checkbox" id="question6" name="question6">
        <label for="question6">Has he gone on the first day?</label>
    </div>
    
    <div class="form-group">
        <input type="submit" value="Check" class="btn btn-primary"/>
    </div>
</form>